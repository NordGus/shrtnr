{{ define "url" }}
    <div
            id="url--{{ .ID }}"
            class="bg-neutral-100 dark:bg-stone-700 p-3 mb-3 rounded drop-shadow-lg"
    >
        <p>ID: {{ .ID }}</p>
        <p>UUID: {{ .UUID }}</p>
        <p>Target: {{ .Target }}</p>
        <p>Created At: {{ .CreatedAt }}</p>
        <p>Deleted At: {{ .DeletedAt }}</p>
    </div>
{{ end }}
